#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_install:
	mkdir -p $(PROJECT_DEBIAN_DIR)/usr/bin
	mkdir -p $(PROJECT_DEBIAN_DIR)/lib/systemd/system
	install -m 755 $(PROJECT_ROOT_DIR)/SampleFile.txt $(PROJECT_DEBIAN_DIR)/usr/bin/SampleFile
	install $(PROJECT_ROOT_DIR)/foobar.service $(PROJECT_DEBIAN_DIR)/lib/systemd/system/foobar.service
